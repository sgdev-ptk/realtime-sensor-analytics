# Copy this file to .env and fill values. Do not commit .env

# API key no longer required for local development

# Redis connection string (service name in compose)
REDIS__CONNECTION=redis:6379

# ASP.NET Core HTTPS dev cert mapping inside container
ASPNETCORE_Kestrel__Certificates__Default__Path=/https/aspnetapp.pfx
ASPNETCORE_Kestrel__Certificates__Default__Password=pass123

# Simulator settings (optional)
# Number of sensors and events per second
SIM__SENSORS=10
SIM__RATE=1000
